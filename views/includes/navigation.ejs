<body id="kt_body" class="header-fixed header-mobile-fixed page-loading">
    <div id="kt_header_mobile" class="header-mobile bg-primary header-mobile-fixed">
        <a href="/">
            <img alt="Logo" src="/media/logos/logo-letter-9.png" class="max-h-30px" />
        </a>
        <div class="d-flex align-items-center">
            <button class="btn p-0 burger-icon burger-icon-left ml-4" id="kt_header_mobile_toggle">
                <span></span>
            </button>
            <button class="btn p-0 ml-2" id="kt_header_mobile_topbar_toggle">
                <span class="svg-icon svg-icon-xl">
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px"
                        height="24px" viewBox="0 0 24 24" version="1.1">
                        <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                            <polygon points="0 0 24 0 24 24 0 24" />
                            <path
                                d="M12,11 C9.790861,11 8,9.209139 8,7 C8,4.790861 9.790861,3 12,3 C14.209139,3 16,4.790861 16,7 C16,9.209139 14.209139,11 12,11 Z"
                                fill="#000000" fill-rule="nonzero" opacity="0.3" />
                            <path
                                d="M3.00065168,20.1992055 C3.38825852,15.4265159 7.26191235,13 11.9833413,13 C16.7712164,13 20.7048837,15.2931929 20.9979143,20.2 C21.0095879,20.3954741 20.9979143,21 20.2466999,21 C16.541124,21 11.0347247,21 3.72750223,21 C3.47671215,21 2.97953825,20.45918 3.00065168,20.1992055 Z"
                                fill="#000000" fill-rule="nonzero" />
                        </g>
                    </svg>
                </span>
            </button>
        </div>
    </div>
    <div class="d-flex flex-column flex-root">
        <div class="d-flex flex-row flex-column-fluid page">
            <div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">
                <div id="kt_header" class="header flex-column header-fixed">
                    <div class="header-top">
                        <div class="container">
                            <div class="d-none d-lg-flex align-items-center mr-3">
                                <a href="/" class="mr-20">
                                    <img alt="Logo" src="/media/logos/logo-letter-9.png" class="max-h-35px" />
                                </a>
                            </div>
                            <% if (!isAuthenticated) { %>
                            <a href="/login" class="mt-5">
                                <div class="topbar">
                                    <div class="topbar-item">
                                        <div class="btn btn-icon btn-hover-transparent-white w-auto d-flex align-items-center btn-lg px-2"
                                            id="kt_quick_user_toggle">
                                            <div class="d-flex flex-column text-right pr-3">
                                                <span
                                                    class="text-white opacity-50 font-weight-bold font-size-sm d-none d-md-inline">User</span>
                                                <span
                                                    class="text-white font-weight-bolder font-size-sm d-none d-md-inline">Login
                                                    Here</span>
                                            </div>
                                            <span class="symbol symbol-35">
                                                <span
                                                    class="symbol-label font-size-h5 font-weight-bold text-white bg-white-o-30">U</span>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </a>
                            <% } else { %>
                            <!-- <form action="/logout" method="POST">
                                <button type="submit">Logout</button>
                            </form> -->
                            <a href="/logout" class="mt-5">
                                <div class="topbar">
                                    <div class="topbar-item">
                                        <div class="btn btn-icon btn-hover-transparent-white w-auto d-flex align-items-center btn-lg px-2"
                                            id="kt_quick_user_toggle">
                                            <div class="d-flex flex-column text-right pr-3">
                                                <span
                                                    class="text-white opacity-50 font-weight-bold font-size-sm d-none d-md-inline">Akshit
                                                    Mithaiwala</span>
                                                <span
                                                    class="text-white font-weight-bolder font-size-sm d-none d-md-inline">Log
                                                    out</span>
                                            </div>
                                            <span class="symbol symbol-35">
                                                <span
                                                    class="symbol-label font-size-h5 font-weight-bold text-white bg-white-o-30">A</span>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </a>
                            <% } %>
                        </div>
                    </div>
                    <div class="header-bottom">
                        <div class="container">
                            <div class="header-menu-wrapper header-menu-wrapper-left" id="kt_header_menu_wrapper">
                                <div id="kt_header_menu"
                                    class="header-menu header-menu-left header-menu-mobile header-menu-layout-default">
                                    <ul class="menu-nav">
                                        <li class="menu-item menu-item-rel" data-menu-toggle="hover"
                                            aria-haspopup="true">
                                            <a href="/" class="menu-link">
                                                <span class="menu-text <%= path === '/' ? 'text-primary' : '' %> ">
                                                    <i
                                                        class="flaticon-home-2 <%= path === '/' ? 'text-primary' : '' %>"></i>
                                                    &nbsp;
                                                    Home</span>
                                                <span class="menu-desc">Latest updates &amp; reports</span>
                                            </a>
                                        </li>
                                        <li class="menu-item menu-item-rel" data-menu-toggle="hover"
                                            aria-haspopup="true">
                                            <a href="/products" class="menu-link">
                                                <span
                                                    class="menu-text <%= path === '/products' ? 'text-primary' : '' %> ">
                                                    <i
                                                        class="flaticon2-box <%= path === '/products' ? 'text-primary' : '' %> "></i>
                                                    &nbsp;
                                                    Products</span>
                                                <span class="menu-desc">Recent Products</span>
                                            </a>
                                        </li>
                                        <% if (isAuthenticated) { %>
                                        <li class="menu-item menu-item-rel" data-menu-toggle="hover"
                                            aria-haspopup="true">
                                            <a href="/cart" class="menu-link">
                                                <span class="menu-text <%= path === '/cart' ? 'text-primary' : '' %> ">
                                                    <i
                                                        class="flaticon2-shopping-cart-1 <%= path === '/cart' ? 'text-primary' : '' %>"></i>
                                                    &nbsp;
                                                    Cart</span>
                                                <span class="menu-desc">Kindly let this cart pass</span>
                                            </a>
                                        </li>
                                        <li class="menu-item menu-item-rel" data-menu-toggle="hover"
                                            aria-haspopup="true">
                                            <a href="/order" class="menu-link">
                                                <span class="menu-text <%= path === '/order' ? 'text-primary' : '' %> ">
                                                    <i
                                                        class="
                                                        flaticon2-gift-1 <%= path === '/order' ? 'text-primary' : '' %>"></i>
                                                    &nbsp;
                                                    Order</span>
                                                <span class="menu-desc">Check my orders</span>
                                            </a>
                                        </li>
                                        <li class="menu-item menu-item-rel" data-menu-toggle="hover"
                                            aria-haspopup="true">
                                            <a href="/admin/add-product" class="menu-link">
                                                <span
                                                    class="menu-text <%= path === '/admin/add-product' ? 'text-primary' : '' %> ">
                                                    <i flaticon-open-box
                                                        class="flaticon2-open-box <%= path === '/admin/add-product' ? 'text-primary' : '' %> "></i>
                                                    &nbsp;
                                                    Add Product</span>
                                                <span class="menu-desc">Admin adds products</span>
                                            </a>
                                        </li>
                                        <li class="menu-item menu-item-rel" data-menu-toggle="hover"
                                            aria-haspopup="true">
                                            <a href="/admin/table-product" class="menu-link">
                                                <span
                                                    class="menu-text <%= path === '/admin/table-product' ? 'text-primary' : '' %> ">
                                                    <i flaticon-open-box
                                                        class="flaticon2-box <%= path === '/admin/table-product' ? 'text-primary' : '' %> "></i>
                                                    &nbsp;
                                                    View Products</span>
                                                <span class="menu-desc">Admin views products</span>
                                            </a>
                                        </li>
                                        <% } %>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>